".source.js, .source.jsx, .source.ts":
    "import":
        "prefix": "import"
        "body": "import { ${1} } from '${0}';"
    "import default":
        "prefix": "importd"
        "body": "import ${1} from '${0}';"
    "import as":
        "prefix": "importa"
        "body": "import * as ${1} from '${0}';"
    "export default":
        "prefix": "exportd"
        "body": "export default"
    "require":
        "prefix": "require"
        "body": """
                require('${1:module}');
                """
    "module.exports":
        "prefix": "module"
        "body": """
                module.exports = ${1:name};
                """
